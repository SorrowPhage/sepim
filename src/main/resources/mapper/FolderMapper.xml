<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sepim.springboot.mapper.FolderMapper">


    <select id="getRecent14Data" resultType="com.sepim.springboot.entity.Folder">
        select * from folder where user_id=#{userId} order by id desc limit 14
    </select>
</mapper>
